#include <iostream>

int factorial(int num);

int main(){
   int num1;
   std::cout<<"Enter the number for Factorial: ";
   std::cin>>num1;

   std::cout<<"Factorial is:"<<factorial(num1);
  
   return 0;
}
int factorial(int num){
    
    if(num>1){
        return num*factorial(num -1);
    }
    else{
        return 1;
    }
}
